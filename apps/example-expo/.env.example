# ToggleBox Example App Configuration
# Copy this file to .env and update the values

# API URL - Your ToggleBox API instance
EXPO_PUBLIC_API_URL=http://localhost:3000/api/v1

# Platform name - Used to fetch configs for this platform
EXPO_PUBLIC_PLATFORM=mobile

# Environment name - staging, production, etc.
EXPO_PUBLIC_ENVIRONMENT=staging

# Optional: API Key for internal actions (toggle flags, manage experiments)
# Leave empty for public-only mode
EXPO_PUBLIC_API_KEY=

# Default user ID for demos
EXPO_PUBLIC_USER_ID=demo-user-123
