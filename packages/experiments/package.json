{
  "name": "@togglebox/experiments",
  "version": "0.1.0",
  "description": "ToggleBox Experiments - A/B testing with multiple variants and statistical analysis",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@togglebox/core": "workspace:*",
    "@togglebox/stats": "workspace:*",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.5.1",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
