{
  "name": "@togglebox/sdk-expo",
  "version": "0.1.0",
  "description": "React Native/Expo SDK for ToggleBox - remote configs, feature flags, and A/B experiments with offline support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "togglebox",
    "expo",
    "react-native",
    "remote-config",
    "feature-flags",
    "ab-testing",
    "experiments",
    "offline",
    "mobile"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ulpi-io/togglebox.git",
    "directory": "packages/sdk-expo"
  },
  "homepage": "https://github.com/ulpi-io/togglebox#readme",
  "bugs": {
    "url": "https://github.com/ulpi-io/togglebox/issues"
  },
  "license": "Elastic-2.0",
  "dependencies": {
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-native": "*",
    "react-native-mmkv": "^2.0.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "react-native-mmkv": {
      "optional": true
    }
  },
  "devDependencies": {
    "@togglebox/configs": "workspace:*",
    "@togglebox/core": "workspace:*",
    "@togglebox/experiments": "workspace:*",
    "@togglebox/flags": "workspace:*",
    "@togglebox/sdk": "workspace:*",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "react-native-mmkv": "^3.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
