{
  "name": "@togglebox/sdk-nextjs",
  "version": "0.1.0",
  "description": "Next.js SDK for remote config service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@togglebox/configs": "workspace:*",
    "@togglebox/core": "workspace:*",
    "@togglebox/flags": "workspace:*",
    "@togglebox/experiments": "workspace:*",
    "@togglebox/sdk": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "next": "^14.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.10",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
